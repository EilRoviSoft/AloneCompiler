struct vector2:
	var %x, float64, [%asx]
	var %y, float64, [%asx + 8]

func @add(float64, float64):
	add64f [%spx - 8], [%spx - 16]
	sub64u %spx, 16
	push64 %asx
	ret

func @main():
	var %v, vector2, [%asx]			# automatically replaces on add64u %spx, 16
	push64 %v.y
	push64 %v.x
	call @add(float64, float64)
	ncall @print64f(float64)
